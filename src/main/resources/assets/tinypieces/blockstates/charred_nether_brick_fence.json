    {   "apply": { "model": "tinypieces:block/multipart/charred_nether_brick_fence_post" }},
    {   "when": { "north": true },
      "apply": { "model": "tinypieces:block/multipart/charred_nether_brick_fence_side", "uvlock": true }
    },
    {   "when": { "east": true },
      "apply": { "model": "tinypieces:block/multipart/charred_nether_brick_fence_side", "y": 90, "uvlock": true }
